<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Lend" table="lend" schema="javaee">
        <id name="isbn">
            <column name="isbn" sql-type="varchar(30)" length="30"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="price">
            <column name="price" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(30)" length="30"/>
        </property>
    </class>
</hibernate-mapping>